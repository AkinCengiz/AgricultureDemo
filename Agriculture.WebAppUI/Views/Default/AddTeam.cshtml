@model Agriculture.Entities.Concrete.Team
@{
    ViewData["Title"] = "AddTeam";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Yeni Personel Ekle</h1>
<form method="post">
    @Html.Label("Personel Adı")
    <input type="text" asp-for="FirstName" class="form-control"/>
    <br/>
    @Html.Label("Personel Soyadı")
    <input type="text" asp-for="LastName" class="form-control"/>
    <br/>
    @Html.Label("Personel Mesleği")
    <input type="text" asp-for="Title" class="form-control"/>
    <br/>
    @Html.Label("Personel Resim")
    <input type="text" asp-for="ImageUrl" class="form-control"/>
    <br/>
    @Html.Label("Facebook")
    <input type="text" asp-for="FacebookUrl" class="form-control"/>
    <br/>
    @Html.Label("Instagram")
    <input type="text" asp-for="InstagramUrl" class="form-control"/>
    <br/>
    @Html.Label("Web Site")
    <input type="text" asp-for="WebsiteUrl" class="form-control" />
    <br />
    @Html.Label("Twitter")
    <input type="text" asp-for="TwitterUrl" class="form-control" />
    <br />
    <button class="btn btn-outline-primary">Kaydet</button>
</form>

