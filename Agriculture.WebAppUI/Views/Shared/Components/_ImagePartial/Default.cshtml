@using Agriculture.Entities.Concrete
@model List<Image>

<div class="gallery" id="gallery">
    <div class="container">
        <h3 class="tittle">Resim Galerimiz</h3>
        <p class="heading-btm-w3ls">Hizmetlerimiz ve etkinliklerimizden ilgi çekici görüntüler</p>
        <div class="agile_gallery_grids w3-agile">
            <ul class="clearfix demo">
                @foreach (var item in Model)
                {
                    <li>
                        <div class="gallery-grid1">
                            <img src="@item.Description" alt=" " class="img-responsive"/>
                            <div class="p-mask">
                                <h4>Hasat <span>Zamanı</span></h4>
                                <p>@item.Title</p>
                            </div>
                        </div>
                    </li>
                }
            </ul>
        </div>
    </div>
</div>